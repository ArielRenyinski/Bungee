####languagesystem DFLT dflt;languagesystem latn dflt;languagesystem latn ROM;languagesystem latn CAT;feature locl { # Localized Forms    script latn; # Latin        language ROM; # Romanian            lookup lookup_ROM {                 sub [Scedilla scedilla] by [Scommaaccent scommaaccent];            } lookup_ROM;        language CAT; # Catalan            sub L' periodcentered L by Ldot;            sub l' periodcentered l by ldot;} locl;feature ordn { # Ordinals    sub @ordn_digits a' by ordfeminine;    sub @ordn_digits o' by ordmasculine;} ordn;feature sups { # Superiors     sub @sups_off by @sups;} sups;feature sinf { # Superiors     sub @sinf_off by @sinf;} sinf;feature ss02 { # Round forms    featureNames {        name "Round Forms";        name 1 "Round Forms";	};    lookup salt_A {        sub @salt_A_off by @salt_A;        sub @salt_a_off by @salt_A;    } salt_A;    lookup salt_M {        sub @salt_M_off by @salt_M;        sub @salt_m_off by @salt_M;    } salt_M;    lookup salt_N {        sub @salt_N_off by @salt_N;        sub @salt_n_off by @salt_N;    } salt_N;    lookup salt_W {        sub @salt_W_off by @salt_W;        sub @salt_w_off by @salt_W;    } salt_W;    lookup salt_X {        sub @salt_X_off by @salt_X;        sub @salt_x_off by @salt_X;    } salt_X;    lookup salt_Y {        sub @salt_Y_off by @salt_Y;        sub @salt_y_off by @salt_Y;    } salt_Y;           } ss02;feature ss03 { # Round E    featureNames {        name "Round E";        name 1 "Round E";	};    lookup salt_E {        sub @salt_E_off by @salt_E;        sub @salt_e_off by @salt_E;    } salt_E;} ss03;feature ss04 { # Serifless I    featureNames {        name "Sans-serif I";        name 1 "Sans-serif I";	};    lookup salt_I {        sub @salt_I_off by @salt_I;        sub @salt_i_off by @salt_i;    } salt_I;} ss04;feature ss05 { # Round L    featureNames {        name "Sans-serif L";        name 1 "Sans-serif L";	};    lookup salt_L {        sub @salt_L_off by @salt_L;        sub @salt_l_off by @salt_L;    } salt_L;} ss05;feature ss06 { # Alternate Ampersand    featureNames {        name "Alternate ampersand";        name 1 "Alternate ampersand";	};    lookup salt_ampersand {        sub @salt_ampersand_off by @salt_ampersand;    } salt_ampersand;} ss06;feature ss07 { # Small Apostrophe    featureNames {        name "Small quotes";        name 1 "Small quotes";	};    lookup salt_quote {        sub @salt_quote_off by @salt_quote;    } salt_quote;} ss07;feature ss08 { # Sequential IJ    featureNames {        name "Sequential IJ";        name 1 "Sequential IJ";	};    lookup salt_IJ {        sub @salt_IJ_off by @salt_IJ;        sub @salt_ij_off by @salt_IJ;    } salt_IJ;} ss08;feature salt { # Stylistic alternates    lookup salt_A;                lookup salt_M;                lookup salt_N;                lookup salt_W;                lookup salt_X;                lookup salt_Y;				        lookup salt_E;    lookup salt_I;	lookup salt_L;        lookup salt_IJ;    lookup salt_ampersand;    lookup salt_quote;        lookup indexes {        sub @arrows by @indexes;     } indexes;    lookup salt_indexes {        sub @arrows by @indexes_salt;        sub @indexes by @indexes_salt;     } salt_indexes;    lookup salt_indexes_outline {        sub @indexes by @indexes_outline;        sub @indexes_salt by @indexes_salt_outline;     } salt_indexes_outline;} salt;feature ornm { # Ornaments    sub bullet from @ornm;} ornm;